	if(! isInIFrame ) {
		var jsData = ["showcase-cta","https:\/\/websitedemos.net\/fashion-photography-04\/wp-content\/plugins\/astra-sites-server\/admin\/showcase-cta\/switcher\/dist\/style-main.css?ver=308d53dc52d1fcf8ca1e"];
